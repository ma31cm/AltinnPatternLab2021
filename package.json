{
  "name": "AltinnPatternLab2021",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "pl:build": "patternlab build --config ./patternlab-config.json",
    "pl:help": "patternlab --help",
    "pl:install": "patternlab install --config ./patternlab-config.json",
    "pl:serve": "patternlab serve --config ./patternlab-config.json",
    "pl:version": "patternlab --version",
    "compileSass:altinndigital": "sass ./source/altinndigital/css/style.dist.altinnett.scss ./public/altinndigital/css/style.dist.altinnett.css --style=expanded --embed-sources",
    "compileSass:common": "sass ./source/common/css/style.prototype.common.scss ./public/common/css/style.prototype.common.css --style=expanded --embed-sources",
    "copyCommon": "shx cp -R ./source/common/_patterns/* ./source/altinn.no/_patterns/",
    "copyCommonAltinnDigital": "shx cp -R ./source/common/_patterns/* ./source/altinndigital/_patterns/",
    "pl:altinn": "patternlab serve --config ./source/altinn/patternlab-config.json",
    "pl:altinndigital": "patternlab serve --config ./source/altinndigital/patternlab-config.json",
    "start:altinn": "npm run copyCommon && npm run pl:altinn",
    "start:altinndigital": "npm run copyCommonAltinnDigital & npm run pl:altinndigital",
    "start:common": "patternlab serve --config ./source/common/patternlab-config.json",
    "start": "concurrently \"npm run watch-*\" \"npm run pl:altinn\"",
    "watch-css": "npm run build-css && chokidar \"./source/css/**/*.scss\" -c \"npm run build-css\"",
    "build-css": "npm run compile-sass & npm run autoprefix-css",
    "compile-sass": "sass ./source/css/scss:./source/css/ --style=expanded",
    "autoprefix-css": "postcss ./source/css/*.css --use autoprefixer --replace --map",
    "watch-js": "npm run bundle-js:prototyping && chokidar \"./source/js/prototyping/modules/*.js\" -c \"npm run bundle-js:prototyping\"",
    "bundle-js:prototyping": "node ./scripts/bundle-js.js",
    "bundle-js:production": "node ./scripts/bundle-js.js --production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ma31cm/AltinnPatternLab2021.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ma31cm/AltinnPatternLab2021/issues"
  },
  "homepage": "https://github.com/ma31cm/AltinnPatternLab2021#readme",
  "dependencies": {
    "@pattern-lab/cli": "^5.14.3",
    "@pattern-lab/core": "^5.14.3",
    "@pattern-lab/engine-handlebars": "^5.14.0",
    "@pattern-lab/starterkit-handlebars-vanilla": "^1.4.0",
    "@pattern-lab/uikit-workshop": "^5.14.3"
  },
  "devDependencies": {
    "anchor-js": "^4.3.1",
    "autoprefixer": "^10.2.6",
    "bootstrap": "^4.6.0",
    "bootstrap-datepicker": "^1.9.0",
    "chokidar-cli": "^2.1.0",
    "clipboard": "^2.0.8",
    "concat-with-sourcemaps": "^1.1.0",
    "concurrently": "^6.2.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "jquery-ui": "^1.12.1",
    "jquery-validation": "^1.19.3",
    "jquery-validation-unobtrusive": "^3.2.12",
    "mark.js": "^8.11.1",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "sass": "^1.35.1",
    "shx": "^0.3.3",
    "slick-carousel": "^1.8.1",
    "truncate.js": "^1.1.2",
    "yargs": "^17.0.1"
  }
}
